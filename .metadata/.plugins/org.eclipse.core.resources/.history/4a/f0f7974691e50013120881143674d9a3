package com.hackbulgaria.helloworld;

public class A {
    public static void main(String[] args) {
        System.out.println(isPrime(5));
    }
    
    private static boolean isPrime(int N){
        //for(int i = 2; i < N/2; i++){
            for(int i = 2; i <= Math.sqrt(N); i++){
            if(N % i == 0) return false;
        }
        return true;
    }
}
