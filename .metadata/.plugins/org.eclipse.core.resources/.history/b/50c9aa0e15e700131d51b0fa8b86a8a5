
public class Test {
    public static void main(String[] args){
        int[][] doubleArray = new int[5][4];
        int[][] doubleArr = new int[5][];
        for(int i = 0; i < doubleArr.length; i++){
            doubleArr[i] = new int[4];
        }
        
        System.out.println(Integer.class);
    }
}
